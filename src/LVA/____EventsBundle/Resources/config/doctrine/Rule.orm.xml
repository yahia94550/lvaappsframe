<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LVA\EventsBundle\Entity\Rule" table="rules">
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dtstart" type="string" column="dtstart" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="intervall" type="integer" column="intervall" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="wkst" type="string" column="wkst" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="countt" type="integer" column="countt" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="untill" type="string" column="untill" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bymonth" type="string" column="bymonth" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="byweekno" type="string" column="byweekno" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="byyearday" type="string" column="byyearday" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bymonthday" type="string" column="bymonthday" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="byday" type="string" column="byday" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="byhour" type="string" column="byhour" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="byminute" type="string" column="byminute" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bysecond" type="string" column="bysecond" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bysetpos" type="string" column="bysetpos" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="exdate" type="string" column="exdate" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="freq" type="string" column="freq" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
